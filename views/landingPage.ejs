<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>    Job-Spot  </title>
  <link rel="stylesheet" href="/css/styles2.css">

  <link rel="stylesheet" href="/css/style2.css">
</head>

<body>
  <header>

    <nav
      class="md:mr-auto md:ml-4 md:py-1 md:pl-4 md:border-l md:border-gray-700	flex flex-wrap items-center text-base justify-center">
      <ul>
        <li>
          <a class="mr-5 hover:text-white" href="./">Home</a>

        </li>
        <li>
          <a class="mr-5 hover:text-white" href="./login">Login</a>

        </li>
        <li>
          <a class="mr-5 hover:text-white" href="./create">Create</a>

        </li>

      </ul>
      <!-- <a class="mr-5 hover:text-white">Fourth Link</a> -->
    </nav>
    </div>
  </header>
    <section class="page-content">
        <% jobs.forEach(function(article){ %>
            <div class="blocks">
                <h3 class="heading">
                    <a href="/job-post/<%- article._id %>">
                        <%- article.jobTitle %>
                    </a>
                </h3>
                <h3>
                    <p class="snippet">
                        <%- article.jobDesc %>
                    </p>
                </h3>
            </div>
            <% }); %>

    </section>
    <footer >
        <div class="container px-5 py-8 mx-auto flex items-center sm:flex-row flex-col">
            <h1>Copyright &copy; Job-Spot 2023 ; </h1>
        </div>
      </footer>
    </body>
    </html>